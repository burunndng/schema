{
  "name": "burundanga-st-assessments",
  "version": "1.0.0",
  "private": true,
  "description": "Burundanga - Comprehensive Schema Therapy assessments with AI-powered insights.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "DATABASE_URL=\"file:./dev.db\" node test-server.cjs",
    "start:server": "DATABASE_URL=\"file:./dev.db\" npm run db:push && DATABASE_URL=\"file:./dev.db\" npm run dev:server",
    "start:full": "concurrently \"npm run start:server\" \"npm run dev\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "db:setup": "ts-node setup-db.ts",
    "db:seed": "ts-node prisma/seed.ts",
    "db:push": "DATABASE_URL=\"file:./dev.db\" prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@google/genai": "^1.29.0",
    "@prisma/client": "^6.19.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "prisma": "^6.19.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.0",
    "@types/pg": "^8.15.6",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
